#include "libft.h"

void    *ft_calloc(size_t count, size_t size)
{
    void    *ptr;
    size_t  total_size;

    if (size != 0 && count > SIZE_MAX / size)
        return (0);

    
    if (count == 0 || size == 0)
        return (malloc(1));

   
    total_size = count * size;

   
    ptr = malloc(total_size);
    if (!ptr)
        return (0);

    
    ft_memset(ptr, 0, total_size);

    return (ptr);
}

