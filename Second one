#include "libft.h"

int	ft_strncmp(const char *s1, const char *s2, size_t n)
{
	size_t	i;

	i = 0;
	if (n == 0)
	{
		return (0);
	}
	while (i < n - 1 && s1[i] == s2[i] && s1[i])
	{
		i++;
	}
	return ((unsigned char)s1[i] - (unsigned char)s2[i]);
}

// #include <stdio.h>

// int main(void)
// {
//     printf("%d\n", ft_strncmp("Hello", "Hello", 5));  
//     printf("%d\n", ft_strncmp("Hello", "Helzo", 5)); 
//     printf("%d\n", ft_strncmp("Hel",   "Hello", 5));  
//     printf("%d\n", ft_strncmp("Hello", "Hel",   5));
// }
#include "libft.h"

char	*ft_strnstr(const char *big, const char *little, size_t len)
{
	size_t	i;
	size_t	j;

	if (*little == '\0')
		return ((char *)big);
	i = 0;
	while (big[i] && i < len)
	{
		j = 0;
		while (big[i + j] && little[j] && (i + j) < len && big[i
			+ j] == little[j])
			j++;
		if (little[j] == '\0')
			return ((char *)big + i);
		i++;
	}
	return (0);
}

#include <stdio.h>
#include "libft.h"

int main(void)
{
    char big[] = "HelloWorld";
    char little[] = "World";

    char *result = ft_strnstr(big, little, 10);

    printf("%s",result);
}

#include "libft.h"

char	*ft_strrchr(const char *s, int c)
{
	unsigned char	n;
	int				i;

	if (!s)
		return (0);
	n = (unsigned char)c;
	i = ft_strlen(s);
	while (i >= 0)
	{
		if (s[i] == n)
		{
			return ((char *)s + i);
		}
		i--;
	}
	return (0);
}

