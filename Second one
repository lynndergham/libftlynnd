#include "libft.h"

int	ft_atoi(const char *nptr)
{
	int	res;
	int	i;
	int	sign;

	res = 0;
	i = 0;
	sign = 1;
	while (nptr[i] == ' ' || (nptr[i] >= 9 && nptr[i] <= 13))
	{
		i++;
	}
	if (nptr[i] == '-' || nptr[i] == '+')
	{
		if (nptr[i] == '-')
		{
			sign *= -1;
		}
		i++;
	}
	while (ft_isdigit((unsigned char)nptr[i]))
	{
		res = res * 10 + (nptr[i] - '0');
		i++;
	}
	return (res * sign);
}

#include <stdio.h>
int main()
{

printf("%d\n", ft_atoi("42"));                 // 42
printf("%d\n", ft_atoi("   42"));              // 42
printf("%d\n", ft_atoi("\t\n\r  42"));         // 42
printf("%d\n", ft_atoi("+42"));                // 42
printf("%d\n", ft_atoi("-42"));                // -42
printf("%d\n", ft_atoi("000123"));             // 123
printf("%d\n", ft_atoi("  -0012a42"));         // -12
printf("%d\n", ft_atoi("a42"));                // 0
printf("%d\n", ft_atoi("999999999999999"));    // undefined (42 won't test overflow)
printf("%d\n", ft_atoi(""));                   // 0
printf("%d\n", ft_atoi("   +0"));              // 0
printf("%d\n", ft_atoi("   -0"));              // 0
printf("%d\n", ft_atoi("-+42"));               // 0 (stops at non-digit)
printf("%d\n", ft_atoi("+-42"));               // 0
printf("%d\n", ft_atoi("--42"));               // 0

}

#include "libft.h"

void		ft_bzero(void *s, size_t n)
{
	ft_memset(s, 0, n);
}

