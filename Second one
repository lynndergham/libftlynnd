#include <stddef.h>
size_t  ft_strlen(const char *s)
{
    size_t i;
    i = 0;

    while(s[i])
        i++;
    return(i);
}


// #include <stdio.h>
// int main()
// {
//     printf("%ld",ft_strlen("hdkwj"));
// }
#include "libft.h"

char	*ft_strmapi(char const *s, char (*f)(unsigned int, char))
{
	unsigned int	i;
	char			*str;

	if (!s || !f)
		return (NULL);
	str = malloc(sizeof(char) * (ft_strlen(s) + 1));
	if (!str)
		return (NULL);
	i = 0;
	while (s[i])
	{
		str[i] = f(i, s[i]);
		i++;
	}
	str[i] = '\0';
	return (str);
}

